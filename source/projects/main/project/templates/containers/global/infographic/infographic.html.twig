{% import "macros/template.html.twig" as helper %}
{% import "macros/schema.html.twig" as schema %}

{# Thia is  very simple container that expects a string as a url #}
<{{ config.container_tag|default('figure') }}{{ helper.class_attribute(config, '')|raw }}{{ helper.container_id_attribute(frontender, 'frontender')|raw }} {{ config.schema|default(false) ? schema._(model) }}>
    <img class="image" src="{{ config.media|t }}"" alt="{{ config.media_alt|t }}" />
</{{ config.container_tag|default('figure') }}>
